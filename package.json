{
  "name": "symplicit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000",
    "build": "next build",
    "start": "next build && next start -p 4000",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "test:once": "jest --config=./jest.config.js",
    "test": "jest --config=./jest.config.js --watch",
    "test:code:updateSnapshot": "jest --config=./jest.config.js --coverage -u",
    "cy:run": "cypress run --config pageLoadTimeout=100000",
    "cy:open": "cypress open --config pageLoadTimeout=100000",
    "cypress": "start-server-and-test dev 4000 cy:run",
    "cypress:open": "start-server-and-test dev 4000 cy:open",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' 'cypress/**/*.js' --config ./.prettierrc"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/enzyme": "^3.1.13",
    "@types/jest": "^29.0.0",
    "@types/redux-mock-store": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "axios": "^1.4.0",
    "bootstrap": "^4.5.0",
    "clsx": "^2.1.0",
    "core-js": "^3.33.3",
    "cypress": "^13.5.1",
    "enzyme": "^3.4.1",
    "enzyme-to-json": "^3.3.4",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "next": "14.0.4",
    "normalizr": "^3.2.4",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.49.3",
    "react-number-format": "^5.0.0",
    "react-redux": "^7.2.0",
    "react-redux-loading-bar": "^5.0.0",
    "reactstrap": "^9.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.3",
    "redux-thunk": "^2.3.0",
    "swr": "^2.0.0",
    "ts-jest": "^29.0.0",
    "universal-cookie": "^4.0.3"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.104",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/redux-logger": "^3.0.12",
    "eslint": "^8.50.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^13.5.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-rulesdir": "^0.2.2",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-storybook": "^0.6.14",
